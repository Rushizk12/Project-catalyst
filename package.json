{
  "name": "project-catalyst",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:web": "vite",
    "dev:api": "tsx watch server/index.ts",
    "dev": "concurrently -k -n web,api -c blue,green \"vite\" \"tsx watch server/index.ts\"",
    "dev:tunnel": "concurrently -k -n web,api,tunnel -c blue,green,yellow \"vite\" \"tsx watch server/index.ts\" \"ngrok http 3001\"",
    "tunnel": "ngrok http 3001",
    "build": "vite build",
    "preview": "vite preview",
    "start:api": "node --loader tsx server/index.ts"
  },
  "dependencies": {
    "@google/genai": "^1.14.0",
    "nodemailer": "^6.9.16",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "googleapis": "^140.0.1",
    "helmet": "^7.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "concurrently": "^9.1.2",
    "ngrok": "^5.0.0-beta.2",
    "tsx": "^4.19.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
